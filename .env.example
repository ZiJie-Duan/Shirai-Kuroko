
# ===== Aliyun OSS (.env.example) =====

# Access credentials
OSS_ACCESS_KEY_ID=your_access_key_id_here
OSS_ACCESS_KEY_SECRET=your_access_key_secret_here

# Endpoint (e.g. Beijing)
OSS_ENDPOINT=https://oss-cn-beijing.aliyuncs.com

# Bucket name
OSS_BUCKET_NAME=your_bucket_name_here

# Signed URL default expiration (seconds)
# 86400 = 24 hours
OSS_SIGN_EXPIRES=86400
